{
    "name": "egeloen/ordered-form-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to order your Symfony2 form fields by adding the position option to all fields.",
    "keywords": [ "form", "order" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": ">=2.1",
        "symfony/form": ">=2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Ivory\\OrderedFormBundle": "" }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "target-dir": "Ivory/OrderedFormBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        }
    }
}
